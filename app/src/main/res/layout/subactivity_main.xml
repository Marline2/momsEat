<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:layout_width="match_parent"
  android:layout_height="match_parent">
<!--HEADER-->
  <ImageButton
    android:id="@+id/back_btn"
    android:layout_width="35dp"
    android:layout_height="35dp"
    android:layout_marginTop="24dp"
    android:layout_marginEnd="16dp"
    android:layout_marginRight="16dp"
    android:layout_marginLeft="33dp"
    android:adjustViewBounds="true"
    android:backgroundTint="#00000000"
    android:contentDescription="BACK_BTN"
    android:scaleType="fitCenter"
    android:scaleX="3"
    android:scaleY="3"
    android:src="@drawable/backbuttonicon"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintTop_toTopOf="parent" />

  <TextView
    android:id="@+id/textView"
    android:layout_width="99dp"
    android:layout_height="51dp"
    android:layout_gravity="center"
    android:fontFamily="@font/elice_bold"
    android:text="메뉴"
    android:textAlignment="center"
    android:textSize="38dp"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.076"
    app:layout_constraintLeft_toLeftOf="parent"
    app:layout_constraintRight_toRightOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintVertical_bias="0.035" />
  <!--MENU.TEXT-->
  <View
    android:id="@+id/view2"
    android:layout_width="match_parent"
    android:layout_height="2dp"
    android:background="@color/gray"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.0"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintVertical_bias="0.116" />

  <TextView
    android:id="@+id/textView4"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_gravity="center"
    android:layout_marginStart="52dp"

    android:layout_marginLeft="52dp"
    android:layout_marginTop="24dp"
    android:layout_marginEnd="317dp"
    android:layout_marginRight="317dp"
    android:text="곡류"
    android:textSize="23dp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/textView" />

  <TextView
    android:id="@+id/textView3"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_gravity="center"
    android:layout_marginStart="52dp"

    android:layout_marginLeft="52dp"
    android:layout_marginTop="12dp"
    android:layout_marginEnd="317dp"
    android:layout_marginRight="317dp"
    android:text="육류"
    android:textSize="23dp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/textView4" />

  <TextView
    android:id="@+id/textView5"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_gravity="center"

    android:layout_marginStart="52dp"
    android:layout_marginLeft="52dp"
    android:layout_marginTop="12dp"
    android:layout_marginEnd="303dp"
    android:layout_marginRight="303dp"
    android:text="생선류"
    android:textSize="23dp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.0"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/textView3" />

  <TextView
    android:id="@+id/textView9"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_gravity="center"

    android:layout_marginStart="52dp"
    android:layout_marginLeft="52dp"
    android:layout_marginTop="12dp"
    android:layout_marginEnd="303dp"
    android:layout_marginRight="303dp"
    android:text="유제품"
    android:textSize="23dp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.0"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/textView5" />

  <TextView
    android:id="@+id/textView6"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_gravity="center"

    android:layout_marginStart="52dp"
    android:layout_marginLeft="52dp"
    android:layout_marginTop="12dp"
    android:text="차 &amp; 음료"
    android:textSize="23dp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.0"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/textView9" />

  <androidx.constraintlayout.widget.Guideline
    android:id="@+id/guideline2"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    app:layout_constraintGuide_begin="41dp" />

  <!--HEADER.FAVBTN-->
  <Button
    android:id="@+id/fav1"
    android:layout_width="50dp"
    android:layout_height="37dp"
    android:layout_marginStart="128dp"
    android:layout_marginLeft="128dp"
    android:layout_marginTop="12dp"
    android:background="#00ff0000"
    android:backgroundTint="#00000000"
    android:text="♥"
    android:textSize="13dp"
    app:layout_constraintStart_toEndOf="@+id/textView4"
    app:layout_constraintTop_toBottomOf="@+id/view2"
    tools:ignore="TouchTargetSizeCheck" />

  <Button
    android:id="@+id/button3"
    android:layout_width="50dp"
    android:layout_height="37dp"
    android:layout_marginStart="128dp"
    android:layout_marginLeft="128dp"
    android:layout_marginTop="5dp"
    android:background="#00ff0000"
    android:backgroundTint="#00000000"
    android:text="♥"
    android:textSize="13dp"
    app:layout_constraintStart_toEndOf="@+id/textView3"
    app:layout_constraintTop_toBottomOf="@+id/fav1"
    tools:ignore="TouchTargetSizeCheck" />

  <Button
    android:id="@+id/button4"
    android:layout_width="50dp"
    android:layout_height="37dp"
    android:layout_marginStart="107dp"
    android:layout_marginLeft="107dp"
    android:layout_marginTop="5dp"
    android:background="#00ff0000"
    android:backgroundTint="#00000000"
    android:text="♥"
    android:textSize="13dp"
    app:layout_constraintStart_toEndOf="@+id/textView9"
    app:layout_constraintTop_toBottomOf="@+id/button5"
    tools:ignore="TouchTargetSizeCheck" />

  <Button
    android:id="@+id/button5"
    android:layout_width="50dp"
    android:layout_height="37dp"
    android:layout_marginStart="107dp"
    android:layout_marginLeft="107dp"
    android:layout_marginTop="4dp"
    android:background="#00ff0000"
    android:backgroundTint="#00000000"
    android:text="♥"
    android:textSize="13dp"
    app:layout_constraintStart_toEndOf="@+id/textView5"
    app:layout_constraintTop_toBottomOf="@+id/button3"
    tools:ignore="TouchTargetSizeCheck" />

  <Button
    android:id="@+id/button6"
    android:layout_width="50dp"
    android:layout_height="37dp"
    android:layout_marginStart="81dp"
    android:layout_marginLeft="81dp"
    android:layout_marginTop="5dp"
    android:background="#00ff0000"
    android:backgroundTint="#00000000"
    android:text="♥"
    android:textSize="13dp"

    app:layout_constraintStart_toEndOf="@+id/textView6"
    app:layout_constraintTop_toBottomOf="@+id/button4"
    tools:ignore="TouchTargetSizeCheck" />
  <!--HEADER.HATEBTN-->
  <Button
    android:id="@+id/button7"
    android:layout_width="47dp"
    android:layout_height="43dp"
    android:layout_marginTop="6dp"
    android:layout_marginEnd="60dp"
    android:background="#00ff0000"
    android:layout_marginRight="60dp"
    android:backgroundTint="#00000000"
    android:text="X"
    android:textColor="@color/black"
    android:textSize="18dp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/view2"
    tools:ignore="TouchTargetSizeCheck" />

  <Button
    android:id="@+id/button8"
    android:layout_width="47dp"
    android:layout_height="43dp"
    android:layout_marginEnd="60dp"
    android:layout_marginRight="60dp"
    android:backgroundTint="#00000000"
    android:text="X"
    android:textColor="@color/black"
    android:textSize="18dp"
    android:background="#00ff0000"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/button7"
    tools:ignore="TouchTargetSizeCheck" />

  <Button
    android:id="@+id/button9"
    android:layout_width="47dp"
    android:layout_height="43dp"
    android:layout_marginEnd="60dp"
    android:layout_marginRight="60dp"
    android:backgroundTint="#00000000"
    android:text="X"
    android:textColor="@color/black"
    android:background="#00ff0000"
    android:textSize="18dp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/button8"
    tools:ignore="TouchTargetSizeCheck" />

  <Button
    android:id="@+id/button10"
    android:layout_width="47dp"
    android:layout_height="43dp"
    android:layout_marginEnd="60dp"
    android:layout_marginRight="60dp"
    android:backgroundTint="#00000000"
    android:text="X"
    android:background="#00ff0000"
    android:textColor="@color/black"
    android:textSize="18dp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/button9"
    tools:ignore="TouchTargetSizeCheck" />

  <Button
    android:id="@+id/button11"
    android:layout_width="47dp"
    android:layout_height="43dp"
    android:layout_marginEnd="60dp"
    android:layout_marginRight="60dp"
    android:backgroundTint="#00000000"
    android:text="X"
    android:textColor="@color/black"
    android:textSize="18dp"
    app:layout_constraintEnd_toEndOf="parent"
    android:background="#00ff0000"
    app:layout_constraintTop_toBottomOf="@+id/button10"
    tools:ignore="TouchTargetSizeCheck" />

  <!--SEARCH_RANK-->
  <View
    android:id="@+id/view"
    android:layout_width="match_parent"
    android:layout_height="2dp"
    android:layout_marginTop="20dp"
    android:layout_marginBottom="304dp"
    android:background="@color/gray"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.0"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/textView6"
    app:layout_constraintVertical_bias="0.038" />

  <TextView
    android:id="@+id/textView8"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_gravity="center"
    android:layout_marginTop="24dp"
    android:fontFamily="@font/elice_regular"
    android:text="자주 검색된 음식"
    android:textSize="23dp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.161"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/view" />

</androidx.constraintlayout.widget.ConstraintLayout>